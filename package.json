{
  "name": "landing-pages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -f .env.${ENV:-local} -- next dev --turbopack -p 8060",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint",
    "ngrok": "ngrok http --url=$NGROK_FORWARDING_URL 8060",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:logs": "docker compose logs -f mongodb",
    "db:web": "echo 'Opening Mongo Express at http://localhost:8081' && open http://localhost:8081 || xdg-open http://localhost:8081 || echo 'Please open http://localhost:8081 in your browser'"
  },
  "dependencies": {
    "@umami/node": "^0.4.0",
    "lucide-react": "^0.554.0",
    "mongodb": "^7.0.0",
    "next": "15.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
